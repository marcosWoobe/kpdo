<?xml version="1.0" encoding="UTF-8"?>
<channels>
	<!-- README:
		0 - dynamic, reserved for guilds
		1 - always acts as Party channel, only "name" tag available
		3 - always acts as Rule Violations channel
		9 - acts as Help channel- clientsided message
		65535 - DO NOT CHANGE THE ID- only "name", "enabled", "active" and "logged" tags available
	 -->
	<channel id="1" name="Party"/>
	<channel id="2" name="Staff" logged="yes" access="3" />
	<channel id="3" name="Rule Violations" />
	<channel id="5" name="Game-Chat" logged="yes" level="10"/>
	<channel id="6" name="Trade" level="12" logged="yes" muted="120" conditionId="2" conditionMessage="You may only place one offer in two minutes."/>
	<channel id="7" name="Game-Chat Global" logged="yes"  muted="5" conditionId="4" conditionMessage="You may only place one message in five seconds." level="20"/>
	<channel id="9" name="Help" level="12" logged="yes" muted="120" conditionId="3" conditionMessage="You may say help channel in two minutes."/>
	<channel id="65535" name="Private Chat Channel"  />
	<channel id="65533" name="TV Channel"/>
</channels>
